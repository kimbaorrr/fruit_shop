<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <link href="~/Assets/fontawesome-free-6.1.1-web/css/all.min.css" rel="stylesheet" />
    <link href="~/Assets/Css/main.css" rel="stylesheet" />
    <link href="~/Assets/Css/responsive.css" rel="stylesheet" />
</head>
<body>
    <div id="load">
        <img style=" width: 100%; position: fixed; top: -40%; left: 0; z-index: 999;" src="https://img.idesign.vn/2018/10/23/id-loading-5.gif" />
    </div>
    <div class="navbar navbar-inverse">
        <div class="container header-all">
            <div class="row">
                <div class="col-md-3 logoImg">
                    <a href="">
                        <img src="data:image/webp;base64," />
                    </a>
                </div>
                <div id="menuall" class="col-md-6">
                    <ul class="header-tongthe">
                        <li class="MenuList">@Html.ActionLink("Trang Chủ", "Index", "Home", new { area = "" }, new { @class = "act navbar-brand" })</li>
                        <li class="MenuList">@Html.ActionLink("Cửa Hàng", "Index", "CuaHang", new { area = "" }, new { @class = "act navbar-brand" })</li>
                        <li class="MenuList">@Html.ActionLink("Blog", "Index", "TinTuc", new { area = "" }, new { @class = "act navbar-brand" })</li>
                        <li class="MenuList">@Html.ActionLink("Liên Hệ", "Index", "LienHe", new { area = "" }, new { @class = "act navbar-brand" })</li>
                    </ul>
                </div>
                <div class="col-md-3">
                    <ul class="login-ul">
                        <li>@Html.ActionLink("Đăng Nhập", "Index", "DangNhap", new { area = "" }, new { @class = "setting-login editlogin" })</li>
                        <li>@Html.ActionLink("Đăng Ký", "Index", "DangKy", new { area = "" }, new { @class = "setting-login editlogin" })</li>
                        <li class="gioHang">
                            <a href="@Url.Action("Index", "GioHang")"><img src="~/Assets/Images/gio-hang.jpg" /> 
                                    <span id="gioHang" class="badge badge-danger navbar-badge"></span> 
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <div>
                <div class="row backgr-footer">
                    <div class="col-md-4">
                        <div class="logo-footer">
                            <a href=""><img src="data:image/webp;base64," /></a>
                            <ul>
                                <li>Địa Chỉ: An Phú Đông,Q12,TP.HCM</li>
                                <li>Phone: 037.559.5019</li>
                                <li>Email: Binxtin@gmail.com</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="center-footer">
                            <h6> Hữu Ích:</h6>
                            <ul>
                                <li class="active-header-text">@Html.ActionLink("Tin tức rau sạch", "Index", "Home", new { @class = "black-color" })</li>
                                <li>@Html.ActionLink("YouTube: Rau Sạch Trong Ngày", "Index", "Home", new { @class = "black-color" })</li>
                                <li>Fanpage: Vì Sức Khỏe Người Dùng</li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="map-footer center-footer">

                            <div style="display: flex;">
                                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3918.346396252908!2d106.68940671402463!3d10.861236260594358!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3175283bcfb48b37%3A0xe633a8e78a5a91fa!2zMTksIDYgVsaw4budbiBMw6BpLCBBbiBQaMO6IMSQw7RuZywgUXXhuq1uIDEyLCBUaMOgbmggcGjhu5EgSOG7kyBDaMOtIE1pbmgsIFZp4buHdCBOYW0!5e0!3m2!1svi!2s!4v1667447321810!5m2!1svi!2s" width="600" height="200" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/Assets/scripts/main.js"></script>
</body>
</html>